        "Num=float(input(\"Ingrese el número:\"))\n",
        "if Num%2==0:\n",
        "   print(\"El número ingresado es par\")\n",
        "else:\n",
        "   print(\"El número igresado es impar\")\n",
        "##Ternario\n",
        "Num = float(input(\"Ingrese el número: \"))\n",
        "print(\"El número ingresado es par\" if Num % 2 == 0 else \"El número ingresado es impar\")\n",
        "\n"
