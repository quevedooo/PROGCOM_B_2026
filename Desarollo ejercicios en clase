        "año=int(input(\"Ingres su año\"))\n",
        "if(año%4==0 and año%100 !=0) or (año%400==0):\n",
        "    print(\"El año es bisiesto\")\n",
        "else:\n",
        "    print(\"El año no es bisiesto\")\n",
        "##Ternario\n",
        "año=int(input(\"Ingrese su año: \"))\n",
        "print(\"El año es bisiesto\" if (año % 4 == 0 and año % 100 != 0) or (año % 400 == 0) else \"El año no es bisiesto\")"
